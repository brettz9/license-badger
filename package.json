{
  "name": "license-badger",
  "version": "0.7.2",
  "description": "Builds a badge indicating your project's license(s) and those of its dependencies.",
  "main": "src/index.js",
  "bin": {
    "license-badger": "./bin/index.js",
    "get-license-type": "./bin/get-license-type.js",
    "satisfies": "./bin/satisfies.js"
  },
  "scripts": {
    "get-license-type": "./bin/get-license-type.js",
    "satisfies": "./bin/satisfies.js",
    "build-cli-svg": "clp -c src/optionDefinitions.js cli.svg",
    "help": "./bin/index.js -h",
    "license-badger": "./bin/index.js -l test/fixtures/licenseInfo.json test.svg",
    "coverage-badge": "coveradge --color brightgreen badges/coverage-badge.svg",
    "eslint": "eslint --ext=js,md,html .",
    "test:unit": "rm -rf node_modules/.cache && mocha --require test/bootstrap/node test/** --reporter-options configFile=mocha-multi-reporters.json",
    "test": "npm run eslint && npm run test:unit",
    "test-cov": "nyc --all npm run test:unit && npm run coverage-badge"
  },
  "nyc": {
    "reporter": [
      "html",
      "json-summary",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brettz9/license-badger.git"
  },
  "keywords": [],
  "author": "Brett Zamir",
  "contributors": [],
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "bugs": {
    "url": "https://github.com/brettz9/license-badger/issues"
  },
  "homepage": "https://github.com/brettz9/license-badger#readme",
  "devDependencies": {
    "@mysticatea/eslint-plugin": "^13.0.0",
    "babel-eslint": "^10.0.3",
    "brettz9-missing": "https://github.com/brettz9/license-missing",
    "brettz9-reuseprotective": "https://github.com/brettz9/license-reuseprotective",
    "brettz9-seelicensein": "https://github.com/brettz9/license-seelicensein",
    "brettz9-uncategorized": "https://github.com/brettz9/license-uncategorized",
    "brettz9-unlicensed": "https://github.com/brettz9/license-unlicensed",
    "chai": "^4.2.0",
    "command-line-publish": "^0.6.0",
    "coveradge": "^0.2.0",
    "eslint": "^6.8.0",
    "eslint-config-ash-nazg": "^16.4.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-array-func": "^3.1.3",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-compat": "^3.3.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsdoc": "^20.0.3",
    "eslint-plugin-markdown": "^1.0.1",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^15.0.1",
    "mocha": "^7.0.0",
    "mocha-badge-generator": "https://github.com/brettz9/mocha-badge-generator#updated-badge-up",
    "mocha-multi-reporters": "^1.1.7",
    "nyc": "^15.0.0",
    "q-combinators": "^1.0.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "badge-up": "https://github.com/brettz9/badge-up#npm-and-testing",
    "command-line-basics": "^0.6.2",
    "es6-template-strings": "^2.0.1",
    "licensee": "https://github.com/brettz9/licensee.js#filterPackages",
    "npm-consider": "https://github.com/brettz9/npm-consider#handle-ands"
  }
}
